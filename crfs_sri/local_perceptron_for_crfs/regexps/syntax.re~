^[A-Z][a-z]+$			# Name 
^[A-Z][a-z]+-[A-Z][a-z]+$	# Name-Name
^[a-z][a-z]+-[A-Z][a-z]+$	# name-Name
^[A-Z]+$			# ABC
^[A-Z]$			# C
^[a-z]$ 			# c
^[0-9]+			# 123
^[0-9]$ 			# 3
\- 			# -
^[0-9]+\.[0-9]+$		# 123.123
^[0-9]+\,[0-9]+$		# 123,123
^[A-Z]+\-[0-9]+$ 		# ABC-123
^[A-Z][a-z]+\-[0-9]+$ 		# Abc-123
^[0-9]+(\-[a-z]+)+$ 		# 44-cent-a-barrel, 5-year-old-boy
^[0-9]+\.[0-9]+(\-[a-z]+)+$ 	# 55.55-foot-tall
^[0-9]+\,[0-9]+(\-[a-z]+)+$ 	# 55,55-foot-tall
^[0-9]+\%\-[a-z]+$		# 81%-owned
^[0-9]+th$			# 10th
^[0-9]+\\\/[0-9]+$		# 1\/2
^[0-9]+\\\/[0-9]+\-[a-z]+$	# 1\/2-old
^\'[0-9]+$ 			# '71



# ([^0-9].*[0-9].*|[0-9].*[^0-9].*)
